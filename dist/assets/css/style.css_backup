  position: relative;
  border-radius: 0.25rem;
  min-height: calc(2.5rem + 2px);
  align-items: center;
  justify-content: space-between;
  margin-bottom: 1rem;
}
label.swipe-button input[type="checkbox"] {
  position: absolute;
  opacity: 0;
}
label.swipe-button span {
  width: 3.75rem;
  top: 0;
  right: 0.375rem;
  align-items: center;
  position: relative;
  margin-right: -3.75rem;
  margin-left: 0.5rem;
}
label.swipe-button span:before {
  width: 3.75rem;
  height: 1.6rem;
  border-radius: 10rem;
  background: #e0e0e0;
  content: "";
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  display: block;
  -webkit-transition: 0.2s ease-out;
  -moz-transition: 0.2s ease-out;
  transition: 0.2s ease-out;
}
label.swipe-button span:after {
  left: 2px;
  top: 2.5px;
  right: auto;
  bottom: 1px;
  background: #fff;
  box-shadow: 0px 0px 1rem 0px #eeeeee;
  content: "";
  position: absolute;
  display: block;
  height: 1.25rem;
  width: 1.25rem;
  border-radius: 50%;
  -moz-transition: 0.2s ease-out;
  transition: 0.2s ease-out;
}
label.swipe-button input[type="checkbox"]:checked + span:before {
  background: #514c93;
}
label.swipe-button input[type="checkbox"]:checked + span:after {
  left: calc(100% - 23px);
}
