  border-radius: 50%;
  background-color: #ffffff;
  border: 1px solid#e2e0f6;
}
/* When the radio button is checked, add a blue background */
.radio-div input:checked ~ .img-checkmark {
  background-color: #5753a8;
}
/* Create the indicator (the dot/circle - hidden when not checked) */
.img-checkmark.sign-check:after {
  content: url(../images/signed-unchecked-icon.png);
  position: absolute;
}
.radio-div input:checked ~ .img-checkmark {
  background-color: #5753a8;
}
.radio-div input:checked ~ .sign-check:after {
  content: url(../images/signed-checked-icon.png);
}
.img-checkmark.unsign-check:after {
  content: url(../images/unsigned-icon.png);
  position: absolute;
}
.radio-div input:checked ~ .unsign-check:after {
  content: url(../images/unchecked-unsigned-icon.png);
}
/* Show the indicator (dot/circle) when checked */
.radio-div input:checked ~ .img-checkmark:after {
  display: block;
}
/* Style the indicator (dot/circle) */
.radio-div .img-checkmark:after {
  top: 7px;
  left: 7px;
  border-radius: 50%;
  background: transparent;
}
/* End checkmark style */
