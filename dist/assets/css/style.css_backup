/* Switch style */
.switch-container .switch {
  position: relative;
  display: inline-block;
  width: 100px;
  height: 40px;
}
.switch-container .switch input {
  opacity: 0;
  width: 0;
  height: 0;
}
.switch-container .slider {
  position: absolute;
  cursor: pointer;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: #e2e1ea;
  -webkit-transition: 0.4s;
  transition: 0.4s;
}
.switch-container .slider:before {
  position: absolute;
  content: "";
  height: 26px;
  width: 26px;
  left: 4px;
  bottom: 7px;
  background-color: white;
  -webkit-transition: 0.4s;
  transition: 0.4s;
}
.switch-container .slider:after {
  content: "No";
  position: absolute;
  right: 17px;
  top: 11px;
}
.switch-container input:checked + .slider {
  background-color: #514c93;
}
.switch-container input:focus + .slider {
  box-shadow: 0 0 1px #514c93;
}
.switch-container input:checked + .slider:before {
  -webkit-transform: translateX(64px);
  -ms-transform: translateX(64px);
  transform: translateX(64px);
}
.switch-container input:checked + .slider:after {
  content: "Yes";
  position: absolute;
  left: 17px;
  top: 11px;
  color: #fff;
}
/* Rounded sliders */
.switch-container .slider.round {
  border-radius: 34px;
}
.switch-container .slider.round:before {
  border-radius: 50%;
}

/* End switch style */
/* sign up social media count radio btn style */
.social-count-radio {
  display: block;
